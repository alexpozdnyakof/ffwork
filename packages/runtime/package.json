{
  "name": "@fwork/runtime",
  "version": "1.0.0",
  "description": "framework runtime core",
  "main": "dist/fwork.js",
  "module": "dist/fwork.mjs",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "author": "alexpozdnyakof",
  "license": "MIT",
  "dependencies": {
    "compare-it": "0.0.1",
    "@fwork/dispatcher": "1.0.0",
    "@fwork/router": "1.0.0"
  },
  "scripts": {
    "prepack": "yarn run build",
    "build:dev": "NODE_ENV=development rollup --config-package runtime --bundleConfigAsCjs -c ../../rollup.config.mjs",
    "build:watch": "NODE_ENV=development rollup --config-package runtime --bundleConfigAsCjs -c ../../rollup.config.mjs --watch",
    "build": "NODE_ENV=production rollup --config-package runtime --bundleConfigAsCjs -c ../../rollup.config.mjs",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "vitest --reporter=verbose",
    "test:run": "vitest run"
  }
}
